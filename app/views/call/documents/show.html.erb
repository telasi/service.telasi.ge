<ul class="breadcrumb" >
  <% @customnav.each_with_index do |(k, v), i| %>
  <% if i == @customnav.size - 1%>
    <li><%= k %></li>
  <% else %>
    <li><a href="<%= v %>"><%= k %></a> <span class="divider">/</span></li>
  <% end%>
  <% end %>
</ul>



