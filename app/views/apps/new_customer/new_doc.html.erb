<%= render :partial => 'apps/new_customer/header' %>
<div class="page-header">
  <h3>ახალი ფაილის ატვირთვა</h3>
</div>
<%= simple_form_for(@doc, url: request.url, :html => {:id => 'doc-form', :multipart => true}) do |f| -%>
  <%= f.file_field :file %>
  <div class="form-actions">
    <%= f.submit 'ფაილის ატვირთვა' %>
  </div>
<% end -%>